{
    "manifest_version": 3,
    "name": "Tab Manager for Chrome Dev Docs",
    "version": "1.0",
    
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["src/content.jsx"]
      }
    ],

    "action": {
      "default_popup": "index.html"
    },
    "permissions": ["tabGroups", "scripting"]
    
  }
